# AIR-Py FastAPI Configuration

# Server
HOST=0.0.0.0
PORT=9001
DEBUG=false

# Authentication
AUTH_SHARED_SECRET=change-me

# Redis
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Data Processing
MAX_ROWS_RETURN_JSON=50000
INFER_ROWS=20000
MAX_MEMORY_MB=2048
MAX_WORKERS=2

# File Processing
ALLOWED_EXTENSIONS=.csv,.parquet,.jsonl
TEMP_DIR=/tmp/air-py

# Go Backend
GO_BACKEND_URL=http://localhost:9000
